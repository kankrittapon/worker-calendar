{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "calendar-worker",
	"main": "src/worker.ts",
	"compatibility_date": "2025-11-06",
	"observability": {
		"enabled": true
	},
	"assets": {
		"directory": "public",
		"binding": "ASSETS",
		"html_handling": "auto-trailing-slash"
	},
	"d1_databases": [
		{
			"binding": "calendar_db",
			"database_name": "calendar_db",
			"database_id": "ea0e2b8a-52e5-4001-aa75-0854e85a58c6"
		}
	],
	"triggers": {
		"crons": ["*/5 * * * *"]
	}
}